{
  "compilerOptions": {
    "module": "es2015",
    "sourceMap": true,
    "allowJs": true,
    "target": "esnext",
    "baseUrl": "./",
    "plugins": [
      {
        "name": "@vuedx/typescript-plugin-vue"
      }
    ],
    "moduleResolution": "node",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["cypress-real-events", "node", "cypress"]
  },
  "include": ["cypress/**/*.ts", "src/**/*"],
  "exclude": ["node_modules/**/*.ts"]
}
